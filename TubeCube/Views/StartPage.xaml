<?xml version="1.0" encoding="utf-8" ?>
<views:ViewBase xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:TubeCube.Views"
             xmlns:viewModels="clr-namespace:TubeCube.ViewModels"
             x:DataType="viewModels:StartPageViewModel"
             x:Class="TubeCube.StartPage"
             PageTitle="{Binding Title}"
             x:TypeArguments="viewModels:StartPageViewModel"
             PageMode="Menu"
             ContentDisplayMode="NavigationBar">
    
    <views:ViewBase.PageIcons>
        <ImageButton
            Style="{StaticResource IconButtonStyle}"
            Source="icnsettings.png"/>
    </views:ViewBase.PageIcons>

    <views:ViewBase.PageContent>
        
    </views:ViewBase.PageContent>
</views:ViewBase>